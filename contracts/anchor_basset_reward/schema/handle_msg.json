{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandleMsg",
  "anyOf": [
    {
      "description": "User's operations return the accrued reward in uusd to the user.",
      "type": "object",
      "required": [
        "claim_rewards"
      ],
      "properties": {
        "claim_rewards": {
          "type": "object",
          "properties": {
            "recipient": {
              "anyOf": [
                {
                  "$ref": "#/definitions/HumanAddr"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      }
    },
    {
      "description": "Owner's operations",
      "type": "object",
      "required": [
        "swap_to_reward_denom"
      ],
      "properties": {
        "swap_to_reward_denom": {
          "type": "object"
        }
      }
    },
    {
      "description": "Owner's operations",
      "type": "object",
      "required": [
        "update_global_index"
      ],
      "properties": {
        "update_global_index": {
          "type": "object"
        }
      }
    },
    {
      "description": "Owner's operations",
      "type": "object",
      "required": [
        "update_user_index"
      ],
      "properties": {
        "update_user_index": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "$ref": "#/definitions/HumanAddr"
            },
            "is_send": {
              "anyOf": [
                {
                  "$ref": "#/definitions/Uint128"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      }
    },
    {
      "description": "Owner's operations",
      "type": "object",
      "required": [
        "update_params"
      ],
      "properties": {
        "update_params": {
          "type": "object",
          "required": [
            "swap_denom"
          ],
          "properties": {
            "swap_denom": {
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    }
  }
}
